    "components": {
        "schemas": {
            "register": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string",
                        "description": "Enter Name"
                    },
                    "countryCode": {
                        "type": "string",
                        "description": "Enter Country Code"
                    },
                    "phoneNumber": {
                        "type": "number",
                        "description": "Enter Phone Number"
                    },
                    "email": {
                        "description": "Enter your Email",
                        "type": "string",
                        "format": "email",
                        "default": "abc@yopmail.com"
                    },
                    "password": {
                        "description": "Enter your Password",
                        "type": "string"
                    },
                    "device_token": {
                        "description": "Enter device token",
                        "type": "string"
                    },
                    "device_type": {
                        "description": "0:Web; 1:Android; 2:iOS; 3:default",
                        "type": "number"
                    },
                    "device_model": {
                        "description": "OS-browser name for web; DEVICE-model for mobile apps",
                        "type": "string"
                    }
                }
            },
            "verifyOtp": {
                "type": "object",
                "properties": {
                    "userId": {
                        "description": "Enter Id",
                        "type": "string"
                    },
                    "otp": {
                        "description": "Enter Otp",
                        "type": "number"
                    },
                    "type": {
                        "description": "1 for number verification",
                        "type": "number"
                    }
                }
            },
            "resendOtp": {
                "type": "object",
                "properties": {
                    "userId": {
                        "description": "Enter your Id",
                        "type": "string"
                    }
                }
            },
            "login": {
                "type": "object",
                "properties": {
                    "email": {
                        "description": "Enter your Email",
                        "type": "string",
                        "format": "email",
                        "default": "abc@yopmail.com"
                    },
                    "password": {
                        "description": "Enter your Password",
                        "type": "string"
                    },
                    "device_token": {
                        "description": "Enter device token",
                        "type": "string"
                    },
                    "device_type": {
                        "description": "0:Web; 1:Android; 2:iOS; 3:default",
                        "type": "number"
                    },
                    "device_model": {
                        "description": "OS-browser name for web; DEVICE-model for mobile apps",
                        "type": "string"
                    }
                }
            },
            "updateProfile": {
                "type": "object",
                "properties": {
                    "userId": {
                        "type": "string",
                        "description": "Enter UserId"
                    },
                    "first_name": {
                        "type": "string",
                        "description": "Enter First Name"
                    },
                    "last_name": {
                        "type": "string",
                        "description": "Enter Last Name"
                    },
                    "country_code": {
                        "type": "string",
                        "description": "Enter Country Code"
                    },
                    "phone_number": {
                        "type": "number",
                        "description": "Enter Phone Number"
                    }
                }
            },
            "forgetPassword": {
                "type": "object",
                "properties": {
                    "email": {
                        "description": "Enter email",
                        "type": "string"
                    }
                }
            },
            "resetPassword": {
                "type": "object",
                "properties": {
                    "userId": {
                        "description": "Enter your userId",
                        "type": "string"
                    },
                    "password": {
                        "description": "Enter your Password",
                        "type": "string"
                    }
                }
            },
            "changePassword": {
                "type": "object",
                "properties": {
                    "oldpassword": {
                        "description": "Enter Old Password",
                        "type": "string"
                    },
                    "newpassword": {
                        "description": "Enter New Password",
                        "type": "string"
                    }
                }
            },
            "editProfile": {
                "type": "object",
                "properties": {
                    "lat": {
                        "description": "Enter latitude",
                        "type": "number"
                    },
                    "lan": {
                        "description": "Enter longitude",
                        "type": "number"
                    },
                    "dob": {
                        "type": "string",
                        "format": "date",
                        "description": "User's date of birth"
                    },
                    "height": {
                        "type": "number",
                        "description": "User's height"
                    },
                    "gender": {
                        "type": "number",
                        "enum": [
                            1,
                            2,
                            3
                        ],
                        "description": "1-male; 2-female; 3-other"
                    },
                    "preference": {
                        "type": "number",
                        "enum": [
                            1,
                            2,
                            3
                        ],
                        "description": "1-men; 2-women; 3-everyone"
                    },
                    "lookingFor": {
                        "type": "number",
                        "enum": [
                            1,
                            2,
                            3,
                            4
                        ],
                        "description": "1-relationship; 2-something casual; 3-not sure; 4-prefer not to say"
                    },
                    "occupation": {
                        "type": "number",
                        "enum": [
                            1,
                            2,
                            3,
                            4
                        ],
                        "description": "1-Student; 2-Unemployed; 3-Employed; 4-Self Employed"
                    },
                    "designation": {
                        "type": "string",
                        "description": "User's job designation"
                    },
                    "highestQualification": {
                        "type": "number",
                        "enum": [
                            1,
                            2,
                            3,
                            4,
                            5
                        ],
